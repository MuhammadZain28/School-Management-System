CREATE VIEW Salary_Report as
SELECT t.name as `Teacher`, net_salary as `Salary` , 
	   bonus as `Bonus`, deduction as `Deduction`, 
	   date_paid as `Payment Date`, Branch_name as `Branch`, branch_id,
		case when paid = 0 THEN 'Paid' else 'Not Paid' END as `Status` FROM salary 
		Natural JOIN teachers t
		NATURAL join Branch</sql><current_tab id="0"/></tab_sql></sqlb_project>
